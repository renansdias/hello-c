{
	"apiVersion": "extensions/v1beta1",
	"kind": "Deployment",
	"metadata": {
		"name": "deployment-c"
	},
	"spec": {
		"replicas": 1,
		"template": {
			"metadata": {
				"labels": {
					"app": "hello-c"
				}
			},
			"spec": {
				"containers": [
					{
						"name": "hello-c",
						"image": "renansdias/hello-c:1.0.0",
						"ports": [
							{
								"containerPort": 3456,
								"hostPort": 3456,
								"name": "helloc"
							}
						]
					}
				]
			}
		}
	}
}